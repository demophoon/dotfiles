{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "go.goplsOptions": {
      "completeUnimported": true
  },
  "yaml.schemas": {
      "kubernetes": "/*.yaml"
  },
  "diagnostic.checkCurrentLine": true
}
